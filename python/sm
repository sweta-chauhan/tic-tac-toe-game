int take_and_put(board **b)
{
  int mv,result;
  MOVE m;
  result=check_board(*b);
  if(result==1)
    {
      printf("Computer Win the game");
      return 1;
    }
  if(result==-1)
    {
    printf("Human Win the game");
    return -1;
    }
  if(result==0)
    {
      printf("Enter your movement : ");
      scanf("%d",&mv);
      if(Isvalid_move(mv)==true)
	{
	  m=mapInt2Move(mv);
	  (*b)->b[m.i][m.j]='X';
	  system("clear");
	  print_game();
	  help_board();
	  print_grid(b);
	}
      else
	{
	printf("Invalid choice\n");
	return 2;
	}
    }
  return 0;
}

